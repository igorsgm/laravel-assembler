{
    "name": "igorsgm/laravel-assembler",
    "description": "ðŸ§° â€¢ An extended Laravel Installer CLI that gives you the power to scaffold a new Laravel project with some neat add-ons and set up a git repository in a single command.",
    "keywords": ["cli", "console", "php", "laravel", "framework", "installer", "artisan", "extended"],
    "homepage": "https://github.com/igorsgm/laravel-assembler",
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Moraes",
            "email": "igor.sgm@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laminas/laminas-text": "^2.10",
        "laravel/installer": "^4.2",
        "ext-json": "*"
    },
    "require-dev": {
        "laravel-zero/framework": "^9.2",
        "laravel/pint": "^1.2",
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/laravel-assembler"]
}
