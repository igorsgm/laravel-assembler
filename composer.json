{
    "name": "igorsgm/laravel-assembler",
    "description": "ðŸ§° â€¢ An extended Laravel Installer CLI that gives you the power to scaffold a new Laravel project with some neat add-ons and set up a git repository in a single command.",
    "keywords": ["framework", "laravel", "installer", "extended","console", "cli"],
    "homepage": "https://igormoraes.com",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Moraes",
            "email": "igor.sgm@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laminas/laminas-text": "^2.8",
        "laravel/installer": "^4.1",
        "ext-json": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "pestphp/pest": "^1.0",
        "laravel-zero/framework": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/laravel-assembler"]
}
