{
    "name": "igorsgm/laravel-vault",
    "description": "Laravel application installer extended.",
    "keywords": ["framework", "laravel", "installer", "extended","console", "cli"],
    "homepage": "https://igormoraes.com",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Moraes",
            "email": "igor.sgm@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laminas/laminas-text": "^2.8",
        "laravel-zero/framework": "^8.4",
        "laravel/installer": "^4.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "pestphp/pest": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["vault"]
}
